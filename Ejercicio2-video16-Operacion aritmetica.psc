Proceso principal
	definir a,b,c,resultado como reales;
	
	escribir "Digite el valor de la letra A: ";
	leer a;
	escribir "Digite el valor de la letra B: ";
	leer b;
	escribir "Digite el valor de la letra C: ";
	leer c;
	
	resultado <- (-b +rc(b^2 - 4*a*c))/(2*a);
	
	escribir "El resultado es: ",resultado;
