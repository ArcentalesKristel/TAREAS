//Ejercicio 3: Intercambiar el valor de 2 variables 

Proceso principal
	definir a,b,aux como entero;
	
	escribir "Digite el valor de la letra a: ";
	leer a;
	escribir "Digite el valor de la letra b: ";
	leer b;
	
	aux <- a;
	a <- b;
	b <- aux;
	
	escribir "El numero de valor es: ",a;
	escribir "El numero de valor es: ",b;
	
	
FinProceso
